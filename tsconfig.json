{
  "compilerOptions": {
    "jsx": "react-jsx",
    "lib": ["es6", "dom", "es2019"],
    "target": "esnext",
    "importHelpers": true,
    "noImplicitAny": true,
    "preserveConstEnums": true,
    "allowJs": false,
    "noImplicitReturns": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "strictFunctionTypes": true,
    "useUnknownInCatchVariables": true
  },
  "extends": "@react-native/typescript-config/tsconfig.json"
}
